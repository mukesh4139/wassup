<h1>Show Note</h1>

<ul>

  <li>
    <strong>Body:</strong>
    <%= @note.body %>
  </li>

  <li>
    <strong>Favorite:</strong>
    <%= @note.favorite %>
  </li>

  <li>
    <strong>Sentiment:</strong>
    <%= @note.sentiment %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.note_path(@conn, :edit, @note) %></span>
<span><%= link "Back", to: Routes.note_path(@conn, :index) %></span>
